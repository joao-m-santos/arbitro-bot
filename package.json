    "@typescript-eslint/parser": "^6.3.0",
    "eslint": "^8.47.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "lint-staged": {
